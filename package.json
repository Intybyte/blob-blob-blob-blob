{
	"private": true,
	"sideEffects": false,
	"scripts": {
		"build": "remix build",
		"dev:remix": "remix watch",
		"dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public --compatibility-flags=nodejs_compat",
		"dev": "npm-run-all build --parallel \"dev:*\"",
		"lint": "eslint src/",
		"lint:fix": "eslint --fix src/",
		"#comment": "-- LOCAL --",
		"migrate:local": "CI=true wrangler d1 migrations apply DB --local",
		"seed": "wrangler d1 execute DB --local --file migrations/seed/seed.sql",
		"clean-run": "rm -rf .wrangler && npm run migrate:local && npm run seed"
	},
	"dependencies": {
		"@remix-run/cloudflare": "2.3.0",
		"@remix-run/cloudflare-pages": "2.3.0",
		"@remix-run/css-bundle": "2.3.0",
		"@remix-run/react": "2.3.0",
		"cross-env": "7.0.3",
		"highlight.js": "11.9.0",
		"isbot": "3.7.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"render2": "1.2.2",
		"zod": "3.22.4",
		"zod-validation-error": "2.1.0"
	},
	"devDependencies": {
		"@blobdevelopment/eslint-config": "0.1.0",
		"@cloudflare/workers-types": "4.20231025.0",
		"@remix-run/dev": "2.3.0",
		"@types/react": "18.2.37",
		"@types/react-dom": "18.2.15",
		"eslint": "8.54.0",
		"npm-run-all": "4.1.5",
		"tailwindcss": "3.3.5",
		"typescript": "5.2.2",
		"wrangler": "3.16.0"
	},
	"engines": {
		"node": ">=16.13.0"
	}
}
